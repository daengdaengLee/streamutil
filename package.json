{
  "name": "utilitystreams",
  "type": "commonjs",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./cjs/index.d.ts",
  "version": "0.2.0",
  "description": "Convenient Streams",
  "scripts": {
    "format": "prettier ./src --write",
    "clean:cjs": "tsc --build ./cjs/tsconfig.json --clean",
    "clean:esm": "tsc --build ./esm/tsconfig.json --clean",
    "clean": "npm run clean:cjs && npm run clean:esm",
    "build:cjs": "tsc --build ./cjs/tsconfig.json",
    "build:esm": "tsc --build ./esm/tsconfig.json",
    "build": "npm run clean && npm run format && npm run build:cjs && npm run build:esm",
    "test": "jest"
  },
  "author": {
    "name": "daengdaenglee",
    "email": "gunho1020@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/daengdaengLee/utilitystreams.git"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./cjs/index.d.ts",
        "default": "./cjs/index.js"
      },
      "import": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      }
    }
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/jest": "^29.5.3",
    "jest": "^29.6.1",
    "prettier": "3.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
